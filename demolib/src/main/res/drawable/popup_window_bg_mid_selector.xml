<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#fff7f4fd" />
            <stroke android:width="0.5dp" android:color="@color/divider_color" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="#fff7f4fd" />
            <stroke android:width="0.5dp" android:color="@color/divider_color" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffffffff" />
            <stroke android:width="0.5dp" android:color="#ffffffff" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ffe8e8e8" />
            <stroke android:width="0.5dp" android:color="#ffe8e8e8" />
        </shape>
    </item>
</selector>