<?xml version="1.0" encoding="utf-8"?>
<apps>
	<!-- sina weibo -->
	<app package="com.sina.weibo">
	    <item name="写微博">
			<action>android.intent.action.VIEW</action>
			<scheme>sinaweibo://sendweibo</scheme>
			<icon>R.drawable.icon_sample</icon>
	    </item>
	    
	    <item name="搜索" >
			<action>android.intent.action.VIEW</action>
			<scheme>sinaweibo://searchall</scheme>
			<icon>R.drawable.icon_sample</icon>
	    </item>
	    
	    <item name="扫一扫" >
			<action>android.intent.action.VIEW</action>
			<scheme>sinaweibo://qrcode</scheme>
			<icon>R.drawable.icon_sample</icon>
	    </item>
	    
	    <item name="热门微博">
			<action>android.intent.action.VIEW</action>
			<scheme>sinaweibo://cardlist?containerid=102803&amp;cache_need=1&amp;count=10</scheme>
			<icon>R.drawable.icon_sample</icon>
	    </item>
	</app>
	
	<!--高德 auton avi  -->
	
	<app package="com.autonavi.minimap">
	    <item name="快速导航">
			<action>android.intent.action.VIEW</action>
			<data>androidamap://openFeature?featureName=DirectNavigation&amp;sourceApplication=softname</data>
			<icon>R.drawable.icon_sample</icon>
	    </item>
	    
	    <item name="线路规划" >
			<action>android.intent.action.VIEW</action>
			<data>androidamap://openFeature?featureName=PathSearch&amp;sourceApplication=softname</data>
			<icon>R.drawable.avatar</icon>
	    </item>
	    
	    <item name="周边搜" >
			<action>android.intent.action.VIEW</action>
			<data>androidamap://openFeature?featureName=AroundSearch&amp;sourceApplication=softname</data>
			<icon>R.drawable.icon_sample</icon>
	    </item>
	    
	    <item name="位置共享">
			<action>android.intent.action.VIEW</action>
			<data>androidamap://openFeature?featureName=LocationShare&amp;sourceApplication=softname</data>
			<icon>R.drawable.avatar</icon>
	    </item>
	</app>
	
	<app package="com.android.camera">
	    <item name="自拍" className="com.android.camera.CameraActivity">
	    </item>
	</app>
</apps>